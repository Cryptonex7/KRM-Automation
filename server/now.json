{
    "version": 2,
    "name": "covid19-analysis",
    "builds": [
        { "src": "*.py", "use": "@liudonghua123/now-flask" , "config": { "maxLambdaSize": "30mb" } }
    ],
    "routes": [
        { "src": "/server", "dest": "/"}
    ],
    "rewrites": [
        {
          "source": "/server",
          "destination": "/"
        },
        {
          "source": "/server/(.*)",
          "destination": "/$1"
        }
      ]
}
